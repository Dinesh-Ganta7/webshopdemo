<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
   
    <parameter name="browserName" value="Chrome"></parameter>
	<test  name="RegistrationTest" enabled="false">
		<classes>
			<class name="testcases.RegistrationTest">
				<methods>
					<include name="register"></include>
				</methods>
			</class>
		</classes>
	</test> <!-- Test -->

	<test name="CreateOrderTest">
		<classes>
			<class name="testcases.CreateOrderTest">
				<methods>
					<include name="login"></include>
					<include name="addToCart"></include>
					<include name = "checkout"></include>
					<include name="logout"></include>
				</methods>
			</class>

		</classes>
	</test>

</suite> <!-- Suite -->
